    "@nestjs/core": "10.4.4",
    "@nestjs/graphql": "12.1.1",
    "graphql-scalars": "1.23.0",
    "graphql-upload-ts": "2.1.2",
    "nestjs-cls": "4.3.0",
    "reflect-metadata": "0.2.2"
  },
  "peerDependencies": {
    "@nestjs/common": "10.x",
    "@nestjs/config": "3.x",
    "@nestjs/core": "10.x",
    "@nestjs/graphql": "12.x",
    "@nestjs/testing": "10.x",
    "graphql-scalars": "1.x",
