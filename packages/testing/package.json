{
  "name": "@feedma/nest-testing",
  "version": "0.0.10-alpha.3",
  "description": "Testing resources for nest services",
  "keywords": [
    "nest",
    "testing"
  ],
  "author": "Eduardo Salazar <eduardo.develop@gmail.com>",
  "homepage": "https://github.com/feedma/feedma-nest-modules#readme",
  "license": "MIT",
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/feedma/feedma-nest-modules.git"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc --build",
    "test": "jest --passWithNoTests",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "publish:dev": "npx yalc publish"
  },
  "bugs": {
    "url": "https://github.com/feedma/feedma-nest-modules/issues"
  },
  "devDependencies": {
    "@nestjs/common": "11.0.12",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.0.12",
    "nestjs-cls": "5.4.2",
    "reflect-metadata": "0.2.2"
  },
  "peerDependencies": {
    "@nestjs/common": "11.x",
    "@nestjs/core": "11.x",
    "@nestjs/testing": "11.x",
    "nestjs-cls": "5.x",
    "reflect-metadata": "^0.2.x"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com"
  }
}
